{
  "name": "pwa-notes",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --mode production",
    "dev": "vite --mode development",
    "prod": "vite --mode production",
    "serve": "vite preview",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
    "lint:scripts": "eslint src/**/*.{js,ts,vue}",
    "lint:scripts:fix": "eslint src/**/*.{js,ts,vue}",
    "lint:styles": "stylelint ./src/**/*.{scss,vue}",
    "lint:styles:fix": "stylelint ./src/**/*.{scss,vue} --fix",
    "prettier": "prettier src/**/*.{js,ts,tsx,vue,json} --write",
    "analyze": "analyze -c ./vite.config.ts -p 8889"
  },
  "dependencies": {
    "@primevue/themes": "^4.2.5",
    "primevue": "^4.2.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^0.8.2",
    "@heroicons/vue": "^2.1.3",
    "@tailwindcss/vite": "^4.0.3",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.0.4",
    "config-federation": "^0.0.10",
    "dexie": "^4.0.4",
    "eslint": "^9.21.0",
    "pinia": "^2.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.26.0",
    "vite": "^5.2.11",
    "vite-bundle-analyzer": "^0.16.2",
    "vite-plugin-pwa": "^0.20.0",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.2.8",
    "vuedraggable": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
